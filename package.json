{
  "name": "aftermeal",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest test/unit",
    "test:integration": "jest test/integration --runInBand",
    "test:e2e": "jest test/e2e --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "schema:sync": "yarn typeorm schema:sync -d ./src/database/typeorm/data-source.ts",
    "schema:drop": "yarn typeorm schema:drop -d ./src/database/typeorm/data-source.ts",
    "migration:create": "yarn typeorm migration:create ./src/database/migrations/Migration",
    "migration:generate": "yarn typeorm migration:generate ./src/database/migrations/Migration -d ./src/database/typeorm/data-source.ts",
    "migration:run": "yarn typeorm migration:run -d ./src/database/typeorm/data-source.ts",
    "migration:revert": "yarn typeorm migration:revert -d ./src/database/typeorm/data-source.ts"
  },
  "dependencies": {
    "@js-joda/core": "^5.6.1",
    "@js-joda/timezone": "^2.21.1",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/cli": "^10.2.1",
    "@nestjs/common": "^10.3.9",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.9",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^10.2.9",
    "@nestjs/schedule": "^4.0.2",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/typeorm": "^10.0.2",
    "@types/uuid": "^10.0.0",
    "bcrypt": "^5.1.1",
    "cache-manager": "^5.4.0",
    "cache-manager-redis-store": "^3.0.1",
    "cache-manager-redis-yet": "^4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "mustache": "^4.2.0",
    "mysql2": "^3.9.8",
    "nest-winston": "^1.9.7",
    "nodemailer": "6.9.9",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "ts-jenum": "^2.2.2",
    "typeorm": "0.3.20",
    "typeorm-naming-strategies": "^4.1.0",
    "typeorm-transactional": "^0.5.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.2.9",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/mustache": "^4.2.5",
    "@types/node": "^20.8.10",
    "@types/nodemailer": "^6.4.13",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2",
    "webpack": "5.94.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@config(.*)$": "<rootDir>/src/config$1",
      "^@common(.*)$": "<rootDir>/src/common$1"
    }
  }
}
